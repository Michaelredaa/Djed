      "tex_dir": "$project/Texture",
      "renderer": "Arnold",
      "mtls": "All",
      "colorspace": "aces",
      "udim": true

    },
    "renderer": {
      "surface": {
        "arnold":{
          "base": "base",
          "baseColor": "baseColor",
          "metalness": "metalness",
