    "maya_clarisse_presets": {
      "port_num": 55000,
      "geo_type": "Alembic Reference",
      "mtls_from": "Arnold",
      "mtls_to": "Autodesk Standard Surface",
      "colorspace": "aces"
    },
    "create_mtl_tex_presets":
